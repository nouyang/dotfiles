{
    "editor.fontFamily": "'SourceCodePro+Powerline+Awesome Regular','Source Sans Pro', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 16,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 2000,
    "terminal.integrated.fontFamily": "'SourceCodePro+Powerline+Awesome Regular','Source Sans Pro', 'monospace', monospace, 'Droid Sans Fallback'",
    "window.zoomLevel": -1,
    "editor.autoClosingBrackets": true,
    "html.autoClosingTags": false,
    "python.linting.pylintArgs": [
        "--load-plugins=pylint_django"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--ignore=E501,W503"
    ],
    "[markdown]": {
        "editor.wordWrap": "bounded",
        "editor.wordWrapColumn": 100,
        "editor.quickSuggestions": false,
    },
    "spellchecker.checkInterval": 10000,
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--py36"
    ],
    "window.titleBarStyle": "native",
    "workbench.iconTheme": "vscode-icons",
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "beautify.language": {
        "html": [
            "htm", "html", "django-html"
        ]
    },
    "[proto3]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "go.formatTool": "goimports",
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "workbench.colorTheme": "Noctis Hibernus",
    "cSpell.userWords": [
        "anonymize",
        "anonymized",
        "canonicalization",
        "cybersecurity",
        "cybersecurty",
        "executables",
        "reassociated",
        "unencrypted",
        "unintuitive"
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pug",
        "python",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "explorer.confirmDelete": false,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "markdown.preview.fontSize": 18,
}